{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://opendatalayer.org/schemas/v1/events/auth/mfa-enabled.schema.json",
  "title": "MFA Enabled",
  "description": "Fires when a user enables multi-factor authentication",
  "type": "object",
  "properties": {
    "method": {
      "type": "string",
      "enum": ["totp", "sms", "email", "hardware_key", "push"]
    },
    "userId": {
      "type": "string"
    }
  },
  "required": ["method"],
  "additionalProperties": false
}
