{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://opendatalayer.org/schemas/v1/events/onboarding/tour-started.schema.json",
  "title": "Tour Started",
  "description": "Fires when a user begins a guided product tour",
  "type": "object",
  "properties": {
    "tourId": {
      "type": "string"
    },
    "tourName": {
      "type": "string"
    },
    "trigger": {
      "type": "string",
      "enum": ["auto", "manual", "contextual"]
    }
  },
  "required": ["tourId"],
  "additionalProperties": false
}
